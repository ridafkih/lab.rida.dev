{
  "name": "@lab/subagents",
  "version": "0.1.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./browser/*": {
      "types": "./src/browser/*.ts",
      "default": "./src/browser/*.ts"
    },
    "./vision/*": {
      "types": "./src/vision/*.ts",
      "default": "./src/vision/*.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.39",
    "@ai-sdk/moonshotai": "^2.0.3",
    "@ai-sdk/openai": "^3.0.26",
    "@lab/browser-protocol": "workspace:*",
    "ai": "^6.0.77",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@lab/typescript-config": "workspace:*",
    "@types/bun": "^1.3.8",
    "typescript": "^5"
  }
}
